{
  "name": "ember-parallax",
  "version": "0.2.0",
  "description": "Parallax support for Ember.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember try:each",
    "semantic-release": "semantic-release"
  },
  "repository": "https://github.com/mike-north/ember-parallax",
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "author": "Mike North (http://mike.works) <michael.l.north@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "7.6.1",
    "@commitlint/config-conventional": "7.6.0",
    "@commitlint/travis-cli": "7.6.1",
    "@ember/jquery": "0.6.1",
    "@ember/optional-features": "0.7.0",
    "@mike-north/js-lib-renovate-config": "1.1.5",
    "@mike-north/js-lib-semantic-release-config": "1.0.1",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "3.25.0",
    "ember-cli-app-version": "3.2.0",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-eslint": "5.1.0",
    "ember-cli-htmlbars-inline-precompile": "3.0.2",
    "ember-cli-inject-live-reload": "2.0.2",
    "ember-cli-qunit": "4.4.0",
    "ember-cli-shims": "1.2.0",
    "ember-cli-test-loader": "2.2.0",
    "ember-cli-uglify": "3.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.2",
    "ember-resolver": "8.0.2",
    "ember-source": "3.25.1",
    "ember-source-channel-url": "1.2.0",
    "ember-try": "1.4.0",
    "ember-windoc": "0.3.2",
    "eslint-plugin-ember": "9.6.0",
    "eslint-plugin-node": "11.1.0",
    "husky": "1.2.1",
    "loader.js": "4.7.0",
    "semantic-release": "15.13.32"
  },
  "keywords": [
    "ember-addon",
    "parallax",
    "scroll",
    "perspective"
  ],
  "dependencies": {
    "ember-cli-babel": "^7.0.0",
    "ember-cli-htmlbars": "^5.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoUrl": "http://ember-parallax.herokuapp.com"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "./node_modules/.bin/commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "extends": "@mike-north/js-lib-semantic-release-config"
  }
}
